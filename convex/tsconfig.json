{
  "extends": "../..",
  "compilerOptions": {
    "target": "es2021",
    "lib": ["es2021", "dom"],
    "allowJs": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": false,
    "noUncheckedIndexedAccess": true
  },
  "include": [
    "**/*"
  ],
  "exclude": [
    "node_modules",
    "dist",
    ".convex"
  ]
}